<?xml version="1.0"?>

<!--
	https://docs.jboss.org/author/display/AS71/Developer+Guide#DeveloperGuide-JBossDeploymentStructureFile
	https://github.com/jbossas/jboss-as/blob/master/build/src/main/resources/docs/schema/jboss-deployment-structure-1_1.xsd
-->

<jboss-deployment-structure>
	<deployment>
		<dependencies>
			<!-- JCache -->
			<module name="org.infinispan"/>
			<module name="org.infinispan.client.hotrod"/>
			<!-- JAAS: Security-Cache -->
			<module name="org.jboss.as.controller-client"/>
			<module name="org.jboss.dmr"/>
			<!-- RichFaces -->
			<module name="com.google.guava"/>
			<!-- Seam Faces -->
			<module name="org.hibernate"/>
			<!-- Seam International -->
			<module name="org.joda.time"/>
			<module name="org.slf4j"/>
			<!-- DbUnit mit PostgreSQL -->
			<module name="org.postgresql.jdbc"/>
			<!-- DbUnit mit MySQL -->
			<module name="com.mysql.jdbc"/>
		</dependencies>
	</deployment>
</jboss-deployment-structure>
